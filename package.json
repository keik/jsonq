{
  "name": "jsoon",
  "version": "0.0.1",
  "description": "JSON query engine",
  "scripts": {
    "lint": "npm run eslint",
    "test": "npm run mocha",
    "watch": "babel ./src/jsoon.es -o ./lib/jsoon.js -w --blacklist strict",
    "build": "rm -rf ./dist/* ./lib/* && npm run eslint && npm run babel && npm run browserify && npm run uglify && npm run test",
    "-----": "----------------------------------------------------------------",
    "eslint": "eslint ./src/*.es",
    "mocha": "mocha test/tests.js",
    "babel": "babel ./src/jsoon.es -o ./lib/jsoon.js --blacklist strict",
    "browserify": "browserify -e lib/jsoon.js -o dist/jsoon.js -s jsoon",
    "uglify": "uglifyjs ./dist/jsoon.js -o ./dist/jsoon.min.js --comments"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keik/jsoon.git"
  },
  "bugs": {
    "url": "https://github.com/keik/jsoon/issues"
  },
  "homepage": "https://github.com/keik/jsoon#readme",
  "author": {
