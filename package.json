    "email": "k4t0.kei@gmail.com",
    "url": "keik.info"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.20",
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "debug": "^2.2.0",
    "eslint": "^1.0.0",
    "mocha": "^2.2.5",
    "uglifyjs": "^2.4.10",
    "watch": "^0.16.0"
  }
